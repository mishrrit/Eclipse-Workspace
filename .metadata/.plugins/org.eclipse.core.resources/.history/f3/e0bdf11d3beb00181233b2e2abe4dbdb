
public class ZeroAndOne {
	
	public static void main(String args[]) {
		
		String str ="0001110101010";
		char chararray[] = str.toCharArray();
		
		int len =str.length();
		int countofzero =0;
		for(int i=0;i<len;i++) {
			if(chararray[i]=='0') {
				countofzero++;
			}
		}
		
		for(int i=0;i<len;i++) {
			if(i<countofzero) {
				chararray[i]='0';
			}else {
				chararray[i]='1';
			}
		}
		
		for(int i=0; i< len;i++) {
			System.out.print(chararray[i]);
		}
	}

}
